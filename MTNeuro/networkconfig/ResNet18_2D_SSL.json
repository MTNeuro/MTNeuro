{
    "optimizer": "SGD",
    "beta1": 0.9,
    "beta2": 0.999,
    "learning_rate": 0.01,
    "epochs": 1,
    "batch_size": 8,

    "augmentations": 0,

    "model": "ResNet-18",

    "encoder_name": "resnet18",        
    "encoder_weights": "imagenet",     
    "in_channels": 1,
    "classes": 4,
    "n_blocks": 4,
    "start_filters": 32,
    "activation": "relu",
    "normalization": "batch",
    "conv_mode": "same",
    "dim": 2,

    "outputdir": "outputs",
    "outweightfilename": "ResNet18",

    "eval_threshold": 0.5,
    "eval_reduction": "None",

    "seed":0,
    "task_type": "SSL",
    "method": "byol",
    "projector_hidden_size": 4096,
    "projector_output_size": 256,
    "projector_2_hidden_size": 1024,
    "projector_2_output_size": 64,
    "lr_warmup_epochs": 10,
    "base_learning_rate": 0.02,
    "base_momentum": 0.99,

    "opt_momentum": 0.9,
    "weight_decay": 5e-4,
    "myow_base_weight": 1.0,
    "lr_decay": "cosine",
    "lr_poly_decay_n": 1.5,
    "lr_gamma": 0.1,
    "lr_milestone":[],
    "mm_decay": "cosine",
    "transform": ["random_crop"],
    "trans_prob": 0.5

    
    

}
